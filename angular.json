            ],
            "scripts": [],
            "stylePreprocessorOptions": {
              "includePaths": [
                "src/lib/ui/style"
              ]
            }
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "4mb",
                  "maximumError": "6mb"
                },
                {
                  "type": "anyComponentStyle",
